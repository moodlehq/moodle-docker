#!/usr/bin/env bash
set -e

basedir="$( cd "$( dirname "${BASH_SOURCE[0]}" )/../" && pwd )"

$basedir/bin/moodle-docker-compose exec webserver php admin/cli/install_database.php --fullname="Docker moodle" --shortname="docker_moodle" --summary="Docker moodle site" --adminemail="admin@example.com" "$@"
